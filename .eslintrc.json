{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "no-undef": 0,
      "import/no-unresolved": 0,
      "react/jsx-filename-extension": 0,
      "global-require": 0,
      "react/destructuring-assignment": 0,
      "import/prefer-default-export": 1
    }
}
